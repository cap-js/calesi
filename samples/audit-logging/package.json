{
  "name": "@capire/audit-logging",
  "version": "1.0.0",
  "description": "Sample app for showcasing @cap-js/audit-logging",
  "private": true,
  "dependencies": {
    "@cap-js/audit-logging": "*",
    "@capire/incidents": "*"
  },
  "devDependencies": {
    "@cap-js/sqlite": "*"
  },
  "scripts": {
    "add_remote_incidents": "cd gen/srv && npm add @capire/incidents@git://github.com/cap-js/incidents-app",
    "deploy": "mbt build -t gen --mtar mta.tar && cf deploy gen/mta.tar",
    "start": "cds-serve"
  },
  "cds": {
    ">> NOTE <<": "This is only required for running cds w samples/audit-logging in this monorepo setup, not for real projects.",
    "requires": {
      "audit-logging extensions": {
        "model": "@capire/audit-logging"
      }
    }
  }
}
