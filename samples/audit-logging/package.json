{
  "name": "@capire/audit-logging",
  "version": "0.0.1",
  "description": "Sample app for developing @cap-js/audit-logging",
  "dependencies": {
    "@capire/incidents": "*",
    "@cap-js/audit-logging": "*",
    "@sap/cds": "*",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "*"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "@capire/audit-logging": {
        "model": "@capire/audit-logging/db/data-privacy.cds"
      },
      "[development]": {
        "audit-log": {
          "handle": ["READ", "WRITE"]
        }
      }
    }
  },
  "private": true
}
